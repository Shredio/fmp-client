{
	"name": "shredio/fmp-client",
	"type": "library",
	"license": "MIT",
	"require": {
		"php": ">=8.3",
		"symfony/http-client": "^7.3",
		"halaxa/json-machine": "^1.2",
		"league/csv": "^9.24",
		"webmozart/assert": "^1.11"
	},
	"autoload": {
		"psr-4": {
			"Shredio\\FmpClient\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests"
		}
	},
	"scripts": {
		"phpstan": "phpstan analyse",
		"test": "vendor/bin/phpunit tests"
	},
	"require-dev": {
		"phpstan/phpstan": "^2.1",
		"phpunit/phpunit": "^12.2",
		"phpstan/phpstan-webmozart-assert": "^2.0",
		"nette/utils": "^4.0"
	}
}
